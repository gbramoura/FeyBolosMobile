<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FeyBolosMobile" parent="Theme.MaterialComponents.DayNight.DarkActionBar">

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/fey_coral</item>
        <item name="colorPrimaryVariant">@color/fey_dusty_rose</item>
        <item name="colorOnPrimary">@color/fey_rose_quartz</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/fey_dusty_rose</item>
        <item name="colorSecondaryVariant">@color/fey_rose_quartz</item>
        <item name="colorOnSecondary">@color/fey_cream</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>
</resources>